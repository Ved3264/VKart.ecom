pipeline{
      aegent any
      stages{
            stage("test"){
                  steps{
                        echo "test appication success"
                  }
            }
            stage("build")
            {
                  steps{
                        echo "start build app"
                        sh 'docker build -t ved1111/docker-practise:1.0 .'
                        sh "echo $PASS | docker login -u $USER --password-stdin"
                        sh 'docker push ved1111/docker-practise:1.0'
                  }
            }
      }
}